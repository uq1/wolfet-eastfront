levelshots/xposed_cc_automap
{
    nopicmip
    nocompress
    nomipmaps
    {
        clampmap levelshots/xposed_cc.jpg
        depthFunc equal
        rgbGen identity
    }
} 
levelshots/xposed_cc_trans
{
    nopicmip
    nocompress
    nomipmaps
    {
        clampmap levelshots/xposed_cc.jpg
        blendfunc blend
        rgbGen identity
        alphaGen vertex
    }
}
 
